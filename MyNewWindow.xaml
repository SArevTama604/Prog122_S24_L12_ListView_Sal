<Window
    x:Class="Prog122_S24_L12_ListView_Sal.MyNewWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:Prog122_S24_L12_ListView_Sal"
    mc:Ignorable="d"
    Title="MyNewWindow"
    Height="450"
    Width="800">
    <Canvas
        Background="#FFFFAAAA">
        <Label
            Content="MyNewWindow"
            Canvas.Left="142"
            Canvas.Top="23"
            FontSize="72" />
        <Label
            x:Name="lblBusinessName"
            Content="Label"
            Canvas.Left="212"
            Canvas.Top="196"
            FontSize="36" />
        <ListBox
            x:Name="lbNames"
            Height="254"
            Width="237"
            d:ItemsSource="{d:SampleData ItemCount=5}"
            HorizontalAlignment="Left"
            VerticalAlignment="Center" />

    </Canvas>
</Window>
